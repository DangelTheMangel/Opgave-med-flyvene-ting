
public boolean arrayFront9(int[] nums) {
  int size = nums.length;
  if (size <= 4){
    for (int i = 0; i < size; i++){
      if (nums[i] == 9)
        return true;
      else;
    }
  } else {
      for (int j = 0; j < 4; j++) {
        if (nums[j] == 9)
          return true;
        else;
      }
    }
  return false;

}
